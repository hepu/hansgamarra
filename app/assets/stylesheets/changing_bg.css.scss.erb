.changing-bg-container {
  background-color: black;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 0
}

.bg-image {
  width: 100%;
  height: 100%;
  background-size: cover;
  background-repeat: no-repeat;
  position: absolute;
  -webkit-transition: all 1.61833989s ease;
  -moz-transition: all 1.61833989s ease;
  -ms-transition: all 1.61833989s ease;
  -o-transition: all 1.61833989s ease;
  transition: all 1.61833989s ease;
  opacity: 0
}

.active-bg {
  opacity: 1
}

