// Navbar override
$navbar-inverse-bg: rgba(0,0,0,0.61);
$navbar-inverse-link-color: #AAA;
$navbar-inverse-link-active-color: white;
$navbar-inverse-link-hover-color: white;
$navbar-inverse-brand-color: $navbar-inverse-link-active-color;

// Panels
$panel-bg: rgba(0,0,0,0.55);
$panel-footer-bg: rgba(0,0,0,0.61);
$panel-inner-border: rgba(0,0,0,0.61);
$panel-default-heading-bg: rgba(0,0,0,0.61);
$panel-default-border: rgba(0,0,0,0.61);
$panel-default-text: #EEE;

// Styles
.row {
  z-index: 0;
  position: relative;
}
